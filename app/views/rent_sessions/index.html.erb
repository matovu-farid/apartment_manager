<p style="color: green"><%= notice %></p>
<h1>Rent sessions</h1>
<div id="rent_sessions">
  <div class='flex justify-end'>
    <%= link_to "Renew rent",
      renew_rent_path,
      class: "btn btn-outline-primary" %>
    <%= link_to "New rent session",
      new_rent_session_path,
      class: "btn btn-outline-primary" %>
  </div>
  <%= render "rent_session_table", rent_sessions: @rent_sessions, show: true %>
</div>
