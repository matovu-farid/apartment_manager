<div id="<%= dom_id resident, class:'flex flex-row inline' %>">
  <td
    class="
      whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:pl-0
    "
  >
    <%= resident.name %>
  </td>

  <td
    class="
      whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:pl-0
    "
  >
    <%= resident.physicalId %>
  </td>

  <td
    class="
      whitespace-nowrap py-4 pl-4 pr-3 text-sm font-medium text-gray-900 sm:pl-0
    "
  >
    <%= resident.phonenumber %>
  </td>
</div>
