<p style="color: green"><%= notice %></p>
<h1>Payments</h1>
<% if !fullyPaid?(@rent_session)%>
  <div class='d-flex justify-content-end'>
    <%= link_to "New payment",
  new_rent_session_payment_path,
  class: "btn btn-outline-primary" %>
  </div>
<% end %>
<div id="payments">
  <%= render 'payment_table', rent_session: @rent_session, payments: @payments %>
</div>
